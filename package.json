{
  "name": "wrapper-tools",
  "displayName": "Wrapper Tools",
  "description": "Some wrapper commands to help on daily tasks",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/Matheusm25/wrapper-tools"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wrapper-tools.single-quote-wrapper",
        "title": "Single Quote Wrapper",
        "category": "Wrapper Tools"
      },
      {
        "command": "wrapper-tools.double-quote-wrapper",
        "title": "Double Quote Wrapper",
        "category": "Wrapper Tools"
      },
      {
        "command": "wrapper-tools.comma-wrapper",
        "title": "Comma Wraper",
        "category": "Wrapper Tools"
      },
      {
        "command": "wrapper-tools.list-difference",
        "title": "List Difference"
      },
      {
        "command": "wrapper-tools.single-quote-array-wrapper",
        "title": "Single Quote Array Wrapper",
        "category": "Wrapper Tools"
      },
      {
        "command": "wrapper-tools.array-wrapper",
        "title": "Array Wrapper",
        "category": "Wrapper Tools"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
